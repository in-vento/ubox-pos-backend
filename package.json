{
  "name": "ubox-pos-backend",
  "version": "1.0.0",
  "description": "UBOX POS Backend API - Express.js + PostgreSQL + Prisma",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/postgres prisma generate && tsc",
    "migrate": "prisma migrate deploy"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.5",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.9",
    "prisma": "5.22.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}